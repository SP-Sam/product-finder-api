generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int    @id @default(autoincrement())
  description String
  category    String
  price       Float
  imageUrl    String
  website     String
}

model Search {
  id     Int    @id @default(autoincrement())
  search String
}
